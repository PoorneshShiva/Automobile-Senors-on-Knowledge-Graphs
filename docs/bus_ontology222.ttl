@prefix : <http://example.com/car_ontology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# --- Classes ---

:Bus a rdfs:Class ;
    rdfs:label "Communication Bus" .

:CANBus a rdfs:Class ;
    rdfs:subClassOf :Bus ;
    rdfs:label "CAN Bus" ;
    rdfs:comment "High-speed, robust, designed for safety-critical systems." .

:LINBus a rdfs:Class ;
    rdfs:subClassOf :Bus ;
    rdfs:label "LIN Bus" ;
    rdfs:comment "Low-cost, slower, used for non-critical systems." .

:Sensor a rdfs:Class ;
    rdfs:label "Sensor" .

:System a rdfs:Class ;
    rdfs:label "Vehicle System" .

# --- Sensor Subclasses ---

:PositionSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Position Sensor" .
:AcceleratorSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Accelerator Sensor" .
:SpeedSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Speed Sensor" .
:OxygenSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Oxygen Sensor" .
:KnockSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Knock Sensor" .
:PressureSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Pressure Sensor" .
:TemperatureSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Temperature Sensor" .
:LightSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Light Sensor" .
:ClimateSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Climate Sensor" .
:SeatbeltSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Seatbelt Sensor" .
:AirbagSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Airbag Sensor" .
:DoorSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Door/Window/Mirror Position Sensor" .
:RadarSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Radar Sensor" .
:LidarSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Lidar Sensor" .
:CameraSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Camera Sensor" .
:RainSensor a rdfs:Class ; rdfs:subClassOf :Sensor ; rdfs:label "Rain Sensor" .

# --- System Subclasses ---

:EngineSystem a rdfs:Class ; rdfs:subClassOf :System ; rdfs:label "Engine System" .
:DrivetrainSystem a rdfs:Class ; rdfs:subClassOf :System ; rdfs:label "Drivetrain System" .
:SafetySystem a rdfs:Class ; rdfs:subClassOf :System ; rdfs:label "Safety System" .
:ComfortSystem a rdfs:Class ; rdfs:subClassOf :System ; rdfs:label "Comfort System" .
:EnvironmentalSystem a rdfs:Class ; rdfs:subClassOf :System ; rdfs:label "Environmental System" .


# --- Properties ---

:isConnectedToBus a rdf:Property ;
    rdfs:label "is connected to bus" ;
    rdfs:domain :Sensor ;
    rdfs:range :Bus .

:isSubBusOf a rdf:Property ;
    rdfs:label "is sub-bus of" ;
    rdfs:domain :Bus ;
    rdfs:range :Bus .

:controlsSystem a rdf:Property ;
    rdfs:label "controls system" ;
    rdfs:domain :Bus ;
    rdfs:range :System .

:hasSensor a rdf:Property ;
    rdfs:label "has sensor" ;
    rdfs:domain :System ;
    rdfs:range :Sensor .

# --- Individuals and Relationships ---

:CAN a :CANBus .
:LIN a :LINBus .

:LIN :isSubBusOf :CAN .

# --- Bus to System Relationships ---

:CAN :controlsSystem :EngineSystem, :DrivetrainSystem, :SafetySystem, :EnvironmentalSystem .
:LIN :controlsSystem :ComfortSystem .

# --- Explicit Sensor to Bus Relationships (Individuals) ---

:MyPositionSensor a :PositionSensor ; :isConnectedToBus :CAN .
:MyAcceleratorSensor a :AcceleratorSensor ; :isConnectedToBus :CAN .
:MySpeedSensor a :SpeedSensor ; :isConnectedToBus :CAN .
:MyOxygenSensor a :OxygenSensor ; :isConnectedToBus :CAN .
:MyKnockSensor a :KnockSensor ; :isConnectedToBus :CAN .
:MyPressureSensor a :PressureSensor ; :isConnectedToBus :CAN .
:MyTemperatureSensor a :TemperatureSensor ; :isConnectedToBus :CAN .
:MySeatbeltSensor a :SeatbeltSensor ; :isConnectedToBus :CAN .
:MyAirbagSensor a :AirbagSensor ; :isConnectedToBus :CAN .
:MyRadarSensor a :RadarSensor ; :isConnectedToBus :CAN .
:MyLidarSensor a :LidarSensor ; :isConnectedToBus :CAN .
:MyCameraSensor a :CameraSensor ; :isConnectedToBus :CAN .
:MyRainSensor a :RainSensor ; :isConnectedToBus :CAN .

:MyLightSensor a :LightSensor ; :isConnectedToBus :LIN .
:MyClimateSensor a :ClimateSensor ; :isConnectedToBus :LIN .
:MyDoorSensor a :DoorSensor ; :isConnectedToBus :LIN .

# --- System to Sensor Relationships ---

:EngineSystem :hasSensor :MyPositionSensor, :MyOxygenSensor, :MyKnockSensor, :MyPressureSensor, :MyTemperatureSensor .
:DrivetrainSystem :hasSensor :MySpeedSensor, :MyAcceleratorSensor .
:SafetySystem :hasSensor :MySeatbeltSensor, :MyAirbagSensor .
:ComfortSystem :hasSensor :MyLightSensor, :MyClimateSensor, :MyDoorSensor, :MyRainSensor .
:EnvironmentalSystem :hasSensor :MyRadarSensor, :MyLidarSensor, :MyCameraSensor .